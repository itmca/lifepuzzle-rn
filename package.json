    "android:dev": "ENVFILE=.env.develop npx react-native run-android --mode=developdebug",
    "android:dev-release": "ENVFILE=.env.develop npx react-native run-android --mode=developrelease",
    "android:prod": "ENVFILE=.env.production npx react-native run-android --mode=productiondebug",
    "android:prod-release": "ENVFILE=.env.production npx react-native run-android --mode=productionrelease",
    "ios:dev": "npx react-native run-ios --scheme lifepuzzle_dev",
    "ios:prod": "npx react-native run-ios --scheme lifepuzzle_prod",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "prepare": "husky install"
  },
